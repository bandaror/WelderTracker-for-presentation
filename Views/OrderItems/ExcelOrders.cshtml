@model IEnumerable<WelderTracker150722.Models.OrderItemsModel>

@{
    Layout = null;
}

<form asp-controller="OrderItems" asp-action="Upload" method="post" enctype="multipart/form-data">
    <input type="file" name="file"/>
    <hr />
    <button type="submit">Upload</button>
</form>
@if (Model.Count()>0){
    <hr />
    <table cellpadding="0" cellspacing="0" border="1">
        <tr>
            <td>FirNumber</td>
            <td>ItemName</td>
            <td>Amount</td>
            <td>ToCompleteBy</td>
        </tr>
        @foreach(OrderItemsModel item in Model)
        {
            <tr>
            <td>@item.FirNumber</td>
            <td>@item.ItemName</td>
            <td>@item.Amount</td>
            <td>@item.ToCompleteBy</td>
        </tr>
        }
    </table>
}